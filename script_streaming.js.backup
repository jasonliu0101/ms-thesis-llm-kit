        formatted = formatted.replace(/^(\d+)\.\s/gm, '<strong>$1.</strong> ');
        
        // è™•ç†é‡è¦æ¨™é¡Œï¼ˆä»¥å†’è™Ÿçµå°¾ï¼‰
        formatted = formatted.replace(/^([^<\n]+ï¼š)/gm, '<strong style="color: #2c3e50;">$1</strong>');
        
        return formatted;
    }

    formatThinking(thinking) {
        if (!thinking) return '';
        
