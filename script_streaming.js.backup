        }
        if (this.enableSearchCheckbox) {
            this.enableSearchCheckbox.checked = savedSettings.enableSearch !== false; // é è¨­ç‚º true
        }

        console.log('âœ… è¨­å®šè¼‰å…¥å®Œæˆ');
