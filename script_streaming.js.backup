        // æ¢å¾©ç²—é«”æ–‡å­—æ¨™è¨˜
        formatted = formatted.replace(/&lt;!BOLD!&gt;(.*?)&lt;!ENDBOLD!&gt;/g, '<strong>$1</strong>');
        
        // æ¢å¾©é …ç›®ç¬¦è™Ÿåˆ—è¡¨
