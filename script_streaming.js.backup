            if (!response.ok) {
                throw new Error(`ç²å–è™›æ“¬å¼•ç”¨å¤±æ•—: ${response.status}`);
            }
            return await response.json();
        } catch (error) {
            console.error('âŒ ç²å–è™›æ“¬å¼•ç”¨æ•¸æ“šå¤±æ•—:', error);
            return null;
        }
    }

